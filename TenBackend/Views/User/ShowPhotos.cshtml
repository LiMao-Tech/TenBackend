@model IEnumerable<TenBackend.Models.Entities.TenImage>
@{
    ViewBag.Title = "Photos";
    Layout = "~/Views/Shared/_LayoutAdministrator.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Photos</h1>
    </div>
</div>

<div class="row">
    <div class="col-lg-12">
        @foreach (var item in Model)
        {
        <div class="col-xs-6 col-md-3">
            <div class="thumbnail">
                <img class="lazy" src="~/TenImage?id=@item.ID" data-src="~/TenImage?id=@item.ID" alt="photo">
            </div>
            <div class="text-center">Upload Time:&nbsp;@item.UploadTime</div>
        </div>
        }
    </div>
</div>
        
